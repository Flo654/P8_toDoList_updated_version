{
    "name": "saro0h/to-do-list",
    "license": "proprietary",
    "type": "project",
    "autoload": {
        "psr-4": {
            
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            
            "App\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.1.3",
        "composer/package-versions-deprecated": "1.11.99.4",
        "doctrine/doctrine-bundle": "^2.3",
        "doctrine/doctrine-migrations-bundle": "^3.1",
        "doctrine/orm": "^2.7",
        "incenteev/composer-parameter-handler": "^2.0",
        "sensio/framework-extra-bundle": "^5.0",
        "symfony/apache-pack": "^1.0",
        "symfony/asset": "^4.0",
        "symfony/cache": "^4.0",
        "symfony/console": "^4.0",
        "symfony/flex": "^1.0",
        "symfony/form": "^4.0",
        "symfony/framework-bundle": "^4.0",
        "symfony/lts": "^4@dev",
        "symfony/monolog-bundle": "^3.7",
        "symfony/polyfill-apcu": "^1.0",
        "symfony/proxy-manager-bridge": "^4.0",
        "symfony/security-bundle": "^4.0",
        "symfony/security-csrf": "^4.0",
        "symfony/swiftmailer-bundle": "^3.1",
        "symfony/translation": "^4.0",
        "symfony/twig-bundle": "^4.0",
        "symfony/validator": "^4.0",
        "symfony/yaml": "^4.0",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/intl-extra": "^3.3",
        "twig/twig": "^2.12|^3.0"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.4",
        "liip/test-fixtures-bundle": "2.0.0",
        "phpunit/phpunit": "^9.5",
        "symfony/browser-kit": "^4.0",
        "symfony/css-selector": "^4.0",
        "symfony/dotenv": "^4.0",
        "symfony/maker-bundle": "^1.34",
        "symfony/phpunit-bridge": "^4.0",
        "symfony/stopwatch": "^4.0",
        "symfony/web-profiler-bundle": "^4.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "prepare": [
            "php bin/console d:d:d --if-exists -f",
            "php bin/console d:d:c",
            "php bin/console d:s:u --force",
            "php bin/console d:f:l -n"
        ],
        "prepare-test": [
            "php bin/console d:d:d --env='test' --if-exists -f ",
            "php bin/console d:d:c --env='test'",
            "php bin/console d:s:u --force --env='test'",
            "php bin/console d:f:l -n --env='test'"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true
        }
    }
}
